{
  "homepage": "pch/songlist",
  "name": "songlist",
  "engines": {
    "pnpm": ">=3"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "flag-icons": "^6.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "transfer": "rsync --archive --delete build/ $npm_config_deploy_path",
    "transfer-caramel": "rsync --archive --delete --exclude '*pony.json' build/ caramel:/usr/share/nginx/html",
    "deploy": "yarn build && yarn transfer",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "postcss-normalize": "^10.0.1"
  }
}
